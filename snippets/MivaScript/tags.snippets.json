{
	"MvASSIGN": {
		"prefix": [
			"MvASSIGN",
			"<MvASSIGN"
		],
		"body": "<MvASSIGN NAME = \"${1:l.variable}\" VALUE = \"${2:{ ${3:l.value} }}\">",
		"description": "Tag - Assign",
		"scope": "mv"
	},
	"MvCALL": {
		"prefix": [
			"MvCALL",
			"<MvCALL"
		],
		"body": "${1:<MIVA MvCALL_ERROR = \"nonfatal, nodisplay\">\n}<MvCALL METHOD = \"${2|'GET','POST','HEAD','XML','RAW','OPTIONS','PUT','DELETE','TRACE','CONNECT'|}\"\n\t\tACTION = \"${3:https://www.example.com/index.html}\"${4:\n\t\tFIELDS = \"${5:{ ${6:l.sort, l.limit, l.offset} \}}\"}>\n\t\t${7:<MvASSIGN NAME = \"${8:l.response}\" VALUE = \"{ ${8:l.response} $ s.callvalue \}\">}\n</MvCALL>\n${9:<MIVA MvCALL_ERROR = \"fatal, display\">}",
		"description": "Tag - Call",
		"scope": "mv"
	},
	"MvCALLCONTINUE": {
		"prefix": [
			"MvCALLCONTINUE",
			"<MvCALLCONTINUE"
		],
		"body": "<MvCALLCONTINUE />",
		"description": "Tag - CallContinue",
		"scope": "mv"
	},
	"MvCALLSTOP": {
		"prefix": [
			"MvCALLSTOP",
