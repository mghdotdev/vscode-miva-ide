	},
	"MvDO": {
		"prefix": [
			"MvDO",
			"<MvDO"
		],
		"body": "<MvDO FILE = \"$4\" ${2:NAME = \"${3:l.success}\"} VALUE = \"$1\">",
		"description": "Tag - Do",
		"scope": "mv"
	},
